data[file] = {}\n\n            sheet_names = self.get_sheet_names(file_path)\n            for sheet in sheet_names:\n                df = self.read_sheet(file_path, sheet, fillna_value)\n                if df is not None:\n                    parsed_data[file][sheet] = df.to_dict(orient=\"records\")\n\n        return parsed_data\n\n# Usage Example\nif __name__ == \"__main__\":\n    directory = \"/kaggle/working/Rag-pipelines-experiments/data\"  # Change this to your target directory\n    parser = ExcelParser(directory)\n\n    print(f\"Found Excel files: {parser.excel_files}\")\n    \n    parsed_data = parser.parse_all(fillna_value=\"N/A\")\n    \n    # Print parsed data for each file and sheet\n    for file, sheets in parsed_data.items():\n        print(f\"\\n📁 {file}:\")\n        for sheet, data in sheets.items():\n            print(f\"  📄 Sheet: {sheet} -> {len(data)} rows\")\n            # print(f\"\\n{parsed_data}\")\n\n    # Access specific parsed data\n    # Example: parsed_data[\"example.xlsx\"][\"Sheet1\"]\n","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2025-03-15T23:57:39.211966Z","iopub.execute_input":"2025-03-15T23:57:39.212481Z","iopub.status.idle":"2025-03-15T23:57:39.489747Z","shell.execute_reply.started":"2025-03-15T23:57:39.212442Z","shell.execute_reply":"2025-03-15T23:57:39.481473Z"},"collapsed":true,"jupyter":{"outputs_hidden":true}},"outputs":[{"name":"stdout","text":"Found Excel files: ['Lesson 02 - Quiz.xlsx', 'Lesson 04 - Quiz.xlsx', 'Lesson 03 - Quiz.xlsx', 'Exam 01.xlsx', 'Lesson 01 - Quiz.xlsx', 'Lesson 05 - Quiz.xlsx']\n\n📁 Lesson 02 - Quiz.xlsx:\n  📄 Sheet: Question -> 10 rows\n\n{'Lesson 02 - Quiz.xlsx': {'Question': [{'Bank Name': 'N/A', 'Question': 'Surgery is performed by __________.', 'Randomization (Yes/No)': 'Yes', 'Correct Answer Number': 4, 'Answer 1': 'surgeons', 'Feedback 1': 'N/A', 'Answer 2': 'nurses', 'Feedback 2': 'N/A', 'Answer 3': 'surgical technologists', 'Feedback 3': 'N/A', 'Answer 4': 'teams of healthca